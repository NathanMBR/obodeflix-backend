{
  "name": "obodeflix-backend",
  "version": "1.0.0",
  "description": "OBODEflix Backend",
  "main": "./build/index.js",
  "repository": "https://github.com/NathanMBR/obodeflix-backend",
  "author": "Nathan Murillo <nathanmurillodeoliveira@gmail.com>",
  "license": "MIT",
  "scripts": {
    "postinstall": "husky install",
    "build": "concurrently pnpm:build:*",
    "lint": "eslint --fix",
    "build:types": "tsc -p tsconfig.build.json",
    "build:compile": "babel src --extensions .ts --out-dir build --source-maps --copy-files --no-copy-ignored",
    "start": "node --enable-source-maps .",
    "dev": "tsx -r dotenv/config ./src/index.ts"
  },
  "dependencies": {
    "@babel/cli": "^7.18.6",
    "@babel/core": "^7.18.6",
    "@babel/preset-env": "^7.18.6",
    "@babel/preset-typescript": "^7.18.6",
    "@ffmpeg-installer/ffmpeg": "^1.1.0",
    "@ffprobe-installer/ffprobe": "^2.1.2",
    "@prisma/client": "^4.4.0",
    "babel-plugin-module-resolver": "^5.0.0",
    "bcryptjs": "^2.4.3",
    "concurrently": "^7.2.2",
    "cors": "^2.8.5",
    "dist-exiftool": "^10.53.0",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "fluent-ffmpeg": "^2.1.2",
    "jsonwebtoken": "^9.0.0",
    "node-exiftool": "^2.3.0",
    "prisma": "^4.4.0",
    "typescript": "^5.0.2",
    "zod": "^3.19.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.0.3",
    "@commitlint/config-conventional": "^17.0.3",
    "@swc/core": "^1.2.211",
    "@swc/jest": "^0.2.21",
    "@types/babel__core": "^7.1.19",
    "@types/babel__preset-env": "^7.9.2",
    "@types/bcryptjs": "^2.4.2",
    "@types/cors": "^2.8.12",
    "@types/eslint": "^8.4.5",
    "@types/express": "^4.17.14",
    "@types/fluent-ffmpeg": "^2.1.24",
    "@types/jest": "^29.5.0",
    "@types/jsonwebtoken": "^9.0.1",
    "@types/node": "^18.0.3",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^5.30.5",
    "@typescript-eslint/parser": "^5.30.5",
    "eslint": "^8.19.0",
    "eslint-plugin-jest": "^27.2.1",
    "husky": "^8.0.1",
    "jest": "^29.5.0",
    "lint-staged": "^13.0.3",
    "supertest": "^6.2.4",
    "ts-node": "^10.8.2",
    "tsx": "^4.7.0"
  },
  "pnpm": {
    "overrides": {
      "semver@<5.7.2": ">=5.7.2",
      "semver@>=6.0.0 <6.3.1": ">=6.3.1",
      "zod@<=3.22.2": ">=3.22.3",
      "@babel/traverse@<7.23.2": ">=7.23.2"
    }
  }
}
